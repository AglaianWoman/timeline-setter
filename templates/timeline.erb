<head>
  <link href="stylesheets/timeline-setter.css" rel="stylesheet" />
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.4.4/jquery.min.js"></script>
  <script src="javascripts/vendor/jquery-ui-1.8.9.custom.min.js"></script>
  <script src="javascripts/vendor/underscore-min.js"></script>
</head>
<body>

  <div id="timeline">
    <div class="timeline_controls">
      <a href="#" class="timeline_zoom timeline_zoom_in" data-zoom-direction="in">+</a> <a href="#" class="timeline_zoom timeline_zoom_out" data-zoom-direction="out">-</a> &nbsp;&nbsp;&nbsp;&nbsp; <a href="#" class="timeline_scrub timeline_scrub_left" data-scrub-direction="left">&laquo;</a> <a href="#" class="timeline_scrub timeline_scrub_right" data-scrub-direction="right">&raquo;</a>
    </div>
    <div class="series_nav_container"></div>
    
    
    <div class="timeline_notchbar_container">
      <div class="timeline_notchbar"></div>
    </div>
    <div id="timeline_card_scroller">
      <div id="timeline_card_scroller_inner">
        <div class="timeline_arrow_container">
        </div>
      </div>
    </div>
    <div class="css_arrow css_arrow_up" style="display:none;"></div>
    <div id="timeline_card_container"></div>
    
  </div>

  <script type="text/javascript">
    var timelineData = <%= self.to_json %>;
  </script>
  <script id="year_notch_tmpl" type="text/jst">
    <div class="timeline_year_notch year_notch_<%%= timestamp %>" data-timestamp="<%%= timestamp %>"><span class="timeline_year_notch_year_text"><%%= year %></span></div>
  </script>
  <script id="notch_tmpl" type="text/jst">
    <div style="background:<%%= topcolor %>" class="timeline_notch notch_<%%= timestamp %>" data-timestamp="<%%= timestamp %>">&nbsp;</div>
  </script>
  <script id="series_legend_tmpl" type="text/jst">
    <%% _(window.series).each(function(series) { %>
      <div class="series_legend_item"><span class="series_legend_swatch" style="background:<%%= window.series_colors[series] %>">&nbsp;</span> <span class="series_legend_text"><%%= series %></span></div>
    <%% }); %>
  </script>
  <script id="card_tmpl" type="text/jst">
    <div class="item item_active" data-timestamp="<%%= timestamp %>" style="border-top:1px solid <%%= topcolor %>">
      <div class="item_label">
        <%%= event_description %>
      </div>
        <%% if (event_link){ %>
          <div class="doc_img">
            <a target="_blank" href="<%%= event_link %>"><img src="<%%= event_thumbnail %>"></a>
            <div class="read_btn"><a target="_blank" href="<%%= event_link %>">Read More</a></div>
          </div>
        <%% } %>

      <div class="item_year">
        <%%= event_date %>
      </div>
    </div>
  </script>
  <script src="javascripts/timeline-setter.js"></script>
</body>